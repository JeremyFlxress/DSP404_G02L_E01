@{
    ViewBag.Title = "TurnoverTickets";
}

<div style="text-align: center; margin-top: 50px;">
    <h1>TurnoverTickets - Portal</h1>
    <div id="contenidoParcial">
        @Html.Partial("SeleccionUsuario")
    </div>
</div>

<script>function cargarVista(vista) {
        fetch(`/${vista}`)
            .then(response => response.text())
            .then(html => {
                document.getElementById('contenidoParcial').innerHTML = html;
            })
            .catch(error => console.error('Error cargando vista:', error));
    }</script>
